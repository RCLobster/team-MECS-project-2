<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Profile</title>
</head>
<body>
    <video autoplay muted loop id="myVideo">
        <source src="/images/profilepg.mp4" type="video/mp4">
    </video>
    <table class="table table-striped">
        ...
    </table>

    <h1>Welcome {{req.session.user_id}}</h1>

    {{#if playlists.length}}
    <div class="col-md-6 project-list">
        <h3>Current Projects:</h3>

        {{#each playlists as |playlist|}}
        <div class="row mb-2">
            <div class="col-md-8">
                <h4>{{playlist.play_name}}</h4>
            </div>
            <div class="col-md-4">
                <ul>
                    {{#each playlist.songs as | song |}}
                    <li>
                        <p>Title: {{song.title}}</p>
                        <p>Artist: {{song.artist}}</p>
                        <p>Album: {{song.album}}</p>
                    </li>
                    {{/each}}
                </ul>
            </div>
        </div>
        {{/each}}
    </div>
    {{/if}}
    </div>
</body>
</html>